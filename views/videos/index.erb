<h2>Videos</h2>

<a class="btn" href="/videos/new">Add Videos</a>


  <div class="videos">
  <% @videos.each do |video| %>
    <div class="col-lg-12">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h4><%= video.caption %></h4>
        </div>
      <div class="panel-body">

        <p><%= video.video_url %></p>

<!-- <iframe width="560" height="315" src="https://www.youtube.com/embed/<%= @youtube_id %>" frameborder="0" allowfullscreen></iframe> -->

        <!--video player -->
          <div id="player"></div>
         <!--end video player -->

          <a href="/videos/<%= video.id %>" class="btn btn-primary">Watch Video</a>

      </div>
    </div>
  </div>
</div>

<% end %>

<% end %>
</div>